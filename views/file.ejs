<div
	style="
		width: 100%;
		display: flex;
		flex-direction: column;
		
		align-items: flex-start;
	">
	<a
		href="/"
		style="
			font-size: xx-large;
			display: flex;
			justify-content: center;
			align-items: center;
			height: 5rem;
			border: 1px solid black;
			border-bottom: 0px;
			width: 100%;
		"
		>Home</a
	>
	<table
		class="table"
		style="width: 100%; border-collapse: collapse">
		<thead>
			<tr>
				<% header.forEach(header => { %>
				<th
					style="
						border: 1px solid #000;
						padding: 8px;
						text-align: left;
						background-color: #f2f2f2;
					">
					<%= header %>
				</th>
				<% }); %>
			</tr>
		</thead>
		<tbody>
			<% body.forEach(row => { %>
			<tr onclick="createChart('<%= JSON.stringify(row) %>')">
				<% Object.values(row).forEach(value => { %>
				<td style="border: 1px solid #000; padding: 8px; text-align: left">
					<%= value %>
				</td>
				<% }); %>
			</tr>
			<% }); %>
		</tbody>
	</table>

	<div
		style="
			height: 5rem;
			width: 100%;
			display: flex;
			justify-content: space-evenly;
			align-items: center;
		">
		<% if (page > 1) { %>
		<a
			href="/file/<%= fileName %>?page=<%= page - 1 %>"
			style="font-size: x-large"
			>&laquo; Previous</a
		>
		<% } if (page < pageSize) { %>
		<a
			href="/file/<%= fileName %>?page=<%= page + 1 %>"
			style="font-size: x-large"
			>Next &raquo;</a
		>
		<% } %>
	</div>
</div>
