<div class="div">
	<h1>Files</h1>
	<% if (files.length > 0) { files.forEach(file => { %>
	<p
		class="file-names"
		onclick="getFile('<%= file %>')">
		<%= file %>
	</p>
	<% })} else { %>
	<p>No files uploaded yet.</p>
	<% } %>
</div>

<div class="div">
	<h1>Upload File</h1>
	<form
		action="/"
		method="post"
		enctype="multipart/form-data">
		<input
			type="file"
			id="csvFile"
			name="csvFile"
			accept=".csv" />
		<button type="submit">Upload CSV</button>
		<p
			id="errorMessage"
			style="display: none; color: red">
			Please select a CSV file.
		</p>
	</form>
</div>
